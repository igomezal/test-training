{
    "verbose": false,
    "plugins": {
        "istanbub": {
            "dir": "./coverage",
            "reporters": [
                "text-summary",
                "lcov"
            ],
            "include": [
                "**/*.html"
            ],
            "exclude": [
                "**/test/**",
                "**/bower_components/**",
                "**/node_modules/**"
            ]
        }
    }
}
